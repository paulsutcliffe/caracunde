- form_for(@musica) do |f|
  
  - if @musica.errors.any?
    #errorExplanation
      %h2= t('activerecord.errors.messages.musica.invalid_record')
      %ul
      - @musica.errors.full_messages.each do |msg|
        %li= msg
  .field
    = f.label :soundcloud
    %br
    = f.text_area :soundcloud

  .actions
    = f.submit I18n.t('submit')
