= form_for(@patrocinador, :html => { :multipart => true }) do |f|

  - if @patrocinador.errors.any?
    #errorExplanation
      %h2= t('activerecord.errors.messages.patrocinador.invalid_record')
      %ul
      - @patrocinador.errors.full_messages.each do |msg|
        %li= msg
  .field
    = f.label :link
    %br
    = f.text_field :link
  .field
    = f.label :logo
    %br
    = f.file_field :logo

  .actions
    = f.submit I18n.t('submit')
